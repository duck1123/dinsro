{
  "name": "dinsro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "yarn clean && shadow-cljs release app",
    "build-report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "build:sass": "rollup -c",
    "clean": "rimraf public/js",
    "e2e": "shadow-cljs compile e2e && node out/e2e.js",
    "format": "zprint-clj -i \"./src/**/*.{clj,cljs,cljc,edn}\" -o ./ && zprint-clj -i \"./*.edn\" -o ./",
    "lint": "clj-kondo --lint src",
    "lint:sass": "stylelint --fix src/styles",
    "sc": "shadow-cljs",
    "start": "shadow-cljs watch app",
    "test": "shadow-cljs watch test --config-merge \"{:autorun true}\"",
    "test:once": "shadow-cljs compile test && node out/test.js",
    "watch:sass": "rollup -c -w"
  },
  "devDependencies": {
    "clj-kondo": "^2020.11.7",
    "cypress": "^6.3.0",
    "cypress-clojurescript-preprocessor": "^0.1.4",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-cljs-test": "^0.1.0",
    "shadow-cljs": "^2.11.5"
  },
  "dependencies": {
    "@cljs-oss/module-deps": "^1.1.1",
    "@js-joda/core": "^3.2.0",
    "buffer": "^6.0.2",
    "bulma": "^0.9.1",
    "bulma-calendar": "^6.0.9",
    "create-react-class": "^15.7.0",
    "date-and-time": "^0.14.2",
    "highcharts": "^8.2.2",
    "highlight.js": "^10.5.0",
    "hljs": "^6.2.3",
    "jquery": "^3.5.1",
    "marked": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-grid-layout": "0.16.6",
    "react-highlight.js": "1.0.7",
    "react-icons": "2.2.7",
    "reakit": "^1.2.5",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "shadow": "0.0.1",
    "xmlhttprequest": "^1.8.0"
  }
}
